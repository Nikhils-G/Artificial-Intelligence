    "def Toh(n, X, Y, Z):\n",
    "    if n == 0:\n",
    "        return\n",
    "    Toh(n-1, X, Z, Y)\n",
    "    print(\"Move disk\", n, \"from rod\", X, \"to rod\", Y)\n",
    "    Toh(n-1, Z, Y, X)\n",
    "N=int(input(\"Enter No Of Disks :\"))\n",
    "Toh(N, 'A', 'C', 'B')"
   ]
